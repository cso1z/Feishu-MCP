# 飞书应用凭证
FEISHU_APP_ID=your_feishu_app_id_here
FEISHU_APP_SECRET=your_feishu_app_secret_here
FEISHU_BASE_URL=https://open.feishu.cn/open-apis
FEISHU_TOKEN_LIFETIME=7200

# FEISHU_TOKEN_SERVICE_URL: 自定义token服务地址（可选）
# 请求类型: POST
# 请求参数格式: { "userKey": "string" }
# 返回数据格式: { "token": "string", "expire": number (可选, 单位秒) }
FEISHU_TOKEN_SERVICE_URL=https://open.feishu.cn/open-apis

# 服务器配置
PORT=3333

# 日志配置
LOG_LEVEL=info
LOG_SHOW_TIMESTAMP=true
LOG_SHOW_LEVEL=true
LOG_TIMESTAMP_FORMAT=yyyy-MM-dd HH:mm:ss.SSS

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=300
CACHE_MAX_SIZE=100